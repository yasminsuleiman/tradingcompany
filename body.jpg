    // Get the offset position of the navbar
    var offset = window.scrollY;
    // Add the sticky class to the navbar when you reach its scroll position. Remove "sticky" when you leave the scroll position
    function myFunction() {
        // console.log(offset, window.scrollY);
        if (window.pageYOffset >= 83 && offset < window.scrollY) {
            navbar.classList.add("sticky")
            navbar.getElementsByClassName('bottom-header')[0].classList.remove('d-block');
        } else if(offset > window.scrollY) {
            navbar.getElementsByClassName('bottom-header')[0].classList.add('d-block');
            // navbar.classList.remove("sticky");
        }
        if(window.pageYOffset < 10) {
            navbar.classList.remove("sticky");
        }
        offset = window.scrollY
    }

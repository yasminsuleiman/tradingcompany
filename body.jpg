    $('.star').on('click', function(e){
        e.preventDefault();

        $(this).parents('p.stars').find('.active').removeClass('active');
        $(this).addClass('active');

        $('#comment_rating').val($(this).text())
    });

    $('input.qty').on('change', function () {
        const value = parseInt($(this).val());
        const min_qty = $(this).attr('min');
        const max_qty = $(this).attr('max');

        if (value > max_qty) {
            $(this).val(max_qty);
            (function ($, window, document) {
    $(document).ready(function(){
        toastr.options = {
            "positionClass": "toast-bottom-left",
        }

        toastr['warning']('Maximum order quantity reached');

    });
}(window.jQuery, window, document))
        }
        if (value < min_qty) {
            (function ($, window, document) {
    $(document).ready(function(){
        toastr.options = {
            "positionClass": "toast-bottom-left",
        }

        toastr['warning']('You can&#039;t place an order below this quantity');

    });
}(window.jQuery, window, document))
